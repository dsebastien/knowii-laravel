{
  "name": "knowii",
  "private": true,
  "type": "module",
  "license": "GNU AFFERO GENERAL PUBLIC LICENSE Version 3",
  "scripts": {
    "cm": "npm run commit",
    "commit": "cz",
    "dev": "vite",
    "build": "vite build && vite build --ssr",
    "generate:changelog": "conventional-changelog -p angular -i CHANGELOG.md -s -r 0",
    "generate:changelog-recent": "conventional-changelog -p angular | tail -n +3",
    "release": "release-it",
    "release:final": "release",
    "release:alpha": "release-it --preRelease=alpha",
    "release:beta": "release-it --preRelease=beta",
    "release:rc": "release-it --preRelease=rc",
    "sync": "git fetch upstream && git rebase upstream/main && git push --force",
    "tailwind": "tailwind --config ./tailwind.config.js",
    "tsc": "npx tsc --NoEmit",
    "tsc:watch": "npm run tsc -- --watch",
    "tscw": "npm run tsc:watch"
  },
  "devDependencies": {
    "@inertiajs/vue3": "^1.0.14",
    "@primevue/themes": "4.0.5",
    "@release-it/conventional-changelog": "8.0.1",
    "@tailwindcss/forms": "0.5.8",
    "@tailwindcss/typography": "0.5.15",
    "@vitejs/plugin-vue": "^5.0.0",
    "@vue/server-renderer": "^3.3.13",
    "autoprefixer": "10.4.20",
    "axios": "^1.6.4",
    "commitizen": "4.3.0",
    "cz-conventional-changelog": "3.3.0",
    "laravel-vite-plugin": "1.0.5",
    "postcss": "8.4.41",
    "primevue": "4.0.5",
    "release-it": "17.6.0",
    "tailwindcss": "3.4.10",
    "typescript": "5.5.4",
    "vite": "5.4.2",
    "vue": "^3.3.13"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  }
}
