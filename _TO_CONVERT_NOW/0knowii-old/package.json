  "private": true,
  "repository": {
    "type": "git",
    "url": "git+https://github.com/DeveloPassion/knowii.git"
  },
  "bugs": {
    "url": "https://github.com/DeveloPassion/knowii/issues"
  },
  "homepage": "https://github.com/DeveloPassion/knowii",
  "author": "dSebastien <sebastien@developassion.be> (https://dsebastien.net)",
  "scripts": {
    "build": "npm run build:app",
    "build:app": "npx cross-env NODE_ENV=production && npx nx run app:build",
    "build:prod": "npm run build",
    "clean": "npx rimraf ./dist",
    "clean:install": "npx rimraf package-lock.json && npx rimraf ./node_modules && npx rimraf dist && npm install",
    "cm": "npm run commit",
    "commit": "cz",
    "commit:lint": "commitlint --env HUSKY_GIT_PARAMS",
    "db:admin": "docker run --rm -p 5050:5050 thajeztah/pgadmin4",
    "dba": "npm run db:admin",
    "db:clean": "rm -rf ./supabase && npx supabase init --with-vscode-workspace && npx dotenv -e apps/app/.env.local -- ./tools/supabase-set-local-ports.sh && npx dotenv -e apps/app/.env.local -- ./tools/supabase-set-auth-providers.sh && npx dotenv -e apps/app/.env.local -- ./tools/supabase-enable-logging.sh",
    "db:generate:types": "npm run db:types:generate",
    "db:init": "npm run db:migration:deploy && npm run db:seed",
    "db:migration:deploy": "dotenv -e apps/app/.env.local -- echo DB MIGRATION DEPLOY NOT IMPLEMENTED YET",
    "db:reset": "rm -rf ./supabase && npx supabase init --with-vscode-workspace && npm run db:stop && npm run db:clean && npm run db:start && npm run db:init && npm run db:types:generate",
    "predb:seed": "psql postgresql://postgres:postgres@localhost:12346/postgres -f supabase-db-seed.sql",
    "db:seed": "dotenv -e apps/app/.env.local -- echo DB SEED NOT IMPLEMENTED YET",
    "db:setup": "npm run db:reset",
    "db:start": "npx supabase start",
    "db:stop": "npx supabase stop",
    "db:types:generate": "npx supabase gen types --lang=typescript --local > libs/common/src/lib/generated/supabase/supabase.ts && npx prettier --write libs/common/src/lib/generated/supabase/supabase.ts",
    "format": "npx nx format --all",
    "format:all": "npm run format",
    "format:check": "npx nx format:check",
    "format:affected": "npx nx format --affected",
    "generate:changelog": "conventional-changelog -p angular -i CHANGELOG.md -s -r 0",
    "generate:changelog-recent": "conventional-changelog -p angular | tail -n +3",
    "lint": "npm run lint:all",
    "lint:all": "npm run lint:app",
    "lint:app": "nx lint app",
    "graph": "npm run nx:show",
    "migrate": "npx nx migrate latest",
    "nx:show": "nx show project app --web",
    "psql:shell": "psql postgresql://postgres:postgres@localhost:12346/postgres",
    "prepare": "husky || true",
    "release": "release-it",
    "release:final": "release",
    "release:alpha": "release-it --preRelease=alpha",
    "release:beta": "release-it --preRelease=beta",
    "release:rc": "release-it --preRelease=rc",
    "serve": "npm run start:app",
    "serve:prod": "npm run start:app:prod",
    "serve:app:prod": "npm run start:app:prod",
    "start": "npm run start:app",
    "start:verbose": "npm run start:app:verbose",
    "start:app": "FORCE_COLOR=true npx nx dev app | npx pino-pretty --colorize --translateTime",
    "start:app:verbose": "FORCE_COLOR=true npx nx dev app --verbose | npx pino-pretty --colorize",
    "start:prod": "npm run start:app:prod",
    "start:app:prod": "dotenv -e ../../../apps/app/.env.local -- npx nx start app",
    "sync": "git fetch upstream && git rebase upstream/main && git push --force",
    "tailwind": "tailwind --config ./apps/app/tailwind.config.ts",
    "test": "nx affected:test --all --parallel --maxParallel 10",
    "test:all": "npm run test:client && npm run test:client-ui && npm run test:common && npm run test:knowii && npm run test:server",
    "test:client": "npx nx test client",
    "test:client-ui": "npx nx test client-ui",
    "test:common": "npx nx test common",
    "test:knowii": "npx nx test app",
    "test:server": "npx nx test server",
    "test:e2e": "nx e2e app-e2e",
    "test:watch": "npm run test -- --watch",
    "tsc": "npx tsc --noEmit --project ./tsconfig.app.json",
    "tsc:watch": "npm run tsc -- --watch",
    "tscw": "npm run tsc:watch",
    "update": "npm run migrate"
  },
  "dependencies": {
    "@supabase/supabase-js": "2.45.2",
    "@supabase/ssr": "0.5.0",
    "clsx": "2.1.1",
    "date-fns": "3.6.0",
    "diacritics": "1.3.0",
    "immer": "10.1.1",
    "next": "14.2.3",
    "nextjs-progressbar": "0.0.16",
    "pino": "9.3.2",
    "primeicons": "7.0.0",
    "primereact": "10.6.6",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-icons": "5.2.1",
    "tailwind-merge": "2.5.2",
    "tslib": "^2.3.0",
    "zod": "3.23.8",
    "zod-error": "1.5.0",
    "zustand": "4.4.1"
  },
  "devDependencies": {
    "@commitlint/cli": "19.4.0",
    "@commitlint/config-conventional": "19.2.2",
    "@nx/devkit": "19.6.3",
    "@nx/eslint": "19.6.3",
    "@nx/eslint-plugin": "19.6.3",
    "@nx/jest": "19.6.3",
    "@nx/js": "19.6.3",
    "@nx/next": "19.6.3",
    "@nx/playwright": "19.6.3",
    "@nx/react": "19.6.3",
    "@nx/workspace": "19.6.3",
    "@playwright/test": "^1.36.0",
    "@release-it/conventional-changelog": "8.0.1",
    "@swc-node/register": "1.9.2",
    "@swc/cli": "~0.3.12",
    "@swc/core": "~1.5.7",
    "@swc/helpers": "~0.5.11",
    "@testing-library/dom": "10.4.0",
    "@testing-library/react": "16.0.0",
    "@types/diacritics": "1.3.3",
    "@types/jest": "29.5.12",
    "@types/node": "18.16.9",
    "@types/react": "18.3.1",
    "@types/react-dom": "18.3.0",
    "@typescript-eslint/eslint-plugin": "^7.3.0",
    "@typescript-eslint/parser": "^7.3.0",
    "@vercel/remote-nx": "^2.0.0",
    "autoprefixer": "10.4.13",
    "babel-jest": "29.7.0",
    "commitizen": "4.3.0",
    "concurrently": "8.2.2",
    "conventional-changelog-cli": "5.0.0",
    "cross-env": "7.0.3",
    "cz-customizable": "7.0.0",
    "dotenv": "16.4.5",
    "dotenv-cli": "7.4.2",
    "encoding": "0.1.13",
    "esbuild": "^0.19.2",
    "eslint": "~8.57.0",
    "eslint-config-next": "14.2.3",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-import": "2.27.5",
    "eslint-plugin-jsx-a11y": "6.7.1",
    "eslint-plugin-playwright": "^0.15.3",
    "eslint-plugin-react": "7.32.2",
    "eslint-plugin-react-hooks": "4.6.2",
    "husky": "9.0.11",
    "jest": "29.7.0",
    "jest-environment-jsdom": "29.7.0",
    "jest-environment-node": "^29.4.1",
    "nx": "19.6.3",
    "pino-pretty": "11.2.2",
    "postcss": "8.4.38",
    "prettier": "^2.6.2",
    "release-it": "17.6.0",
    "rimraf": "6.0.1",
    "supabase": "1.190.0",
    "tailwindcss": "3.4.10",
    "ts-jest": "^29.1.0",
    "ts-node": "10.9.1",
    "typescript": "5.5.4"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-customizable"
    }
  }
}
